{
    "compilerOptions": {
        "outDir": "./built/",
        "sourceMap": false,
        "strict": true,
        "noImplicitReturns": true,
        "module": "es2015",
        "moduleResolution": "node",
        "target": "es2015",
        "experimentalDecorators": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true,
        "allowSyntheticDefaultImports": true
    },
    "include": [
        "./src/**/*"
    ],
    "exclude": [
        "./dist/*"
    ]
}